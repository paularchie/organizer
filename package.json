{
  "name": "task-manager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "ng serve",
    "client-secure": "ng serve --ssl task-manager --ssl-key key.pem --ssl-cert cert.pem",
    "build-client": "./node_modules/.bin/ng build --prod",
    "build-server:prerender": "./node_modules/.bin/ng run task-manager:server",
    "build-all": "npm run build-client && npm run build-server",
    "build-all:prerender": "npm-run-all build-client build-server build-server:prerender",
    "prerender": "./node_modules/.bin/ts-node ./prerender.ts",
    "prerender:debug": "./node_modules/.bin/ts-node --inspect ./prerender.ts",
    "stats": "webpack-bundle-analyzer dist/client/stats.json",
    "start-server": "./node_modules/.bin/ts-node ./server/server.ts",
    "server": "./node_modules/.bin/nodemon  -w ./ --ext \".ts\" --exec \"npm run start-server\"",
    "csrf-server": "./node_modules/.bin/http-server -c-1 ."
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.0.0",
    "@angular/material-moment-adapter": "6.0.1",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.0.3",
    "@angular/router": "^6.1.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "argon2": "^0.19.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "html": "^1.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.3.1",
    "nodemailer": "^4.6.8",
    "password-validator": "^4.1.1",
    "redis": "^2.8.0",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mongoose": "^5.2.18",
    "@types/node": "^8.9.5",
    "codelyzer": "~4.3.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
